                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:buttonTint="@color/black"
                android:hint="@string/surya_sen"
                android:textColorHint="@color/black"
                android:textSize="@dimen/_18sp" />

            <RadioButton
